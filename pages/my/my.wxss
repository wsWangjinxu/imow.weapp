.userInfo {
  padding: 30rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 34rpx;
  align-items: center;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.userInfo image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

.myOrder {
  background-color: #fff;
  padding: 36rpx;
}

.myOrder .title {
  font-size: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 60rpx;
  align-items: center;
  border-bottom: 1px solid #cbcbcb;
  margin-top: -10rpx;
  padding-bottom: 20rpx;
}

.myOrder .title navigator {
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.ctn {
  display: flex;
  flex-direction: row;
  margin-bottom: -10rpx;
}

.ctn navigator {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10rpx;
  font-size: 50rpx;
}

.ctn navigator text {
  font-size: 30rpx;
}

.my .cell {
  margin-top: 20rpx;
}